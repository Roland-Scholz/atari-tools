<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project Obj2Com">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <!--define folder properties-->
    <property name="dir.buildfile" value="."/>
    <property name="dir.workspace" value="C:/Users/rolan/workspace"/>
    <property name="dir.jarfile" value="C:/Users/rolan/atari/Obj2Com/bin"/>
    <target name="create_run_jar">
        <jar destfile="${dir.jarfile}/Obj2Com.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="Obj2Com"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${dir.jarfile}"/>
        </jar>
    </target>
</project>
